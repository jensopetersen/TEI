<?xml-model href="http://tei.it.ox.ac.uk/jenkins/job/TEIP5/lastSuccessfulBuild/artifact/release/xml/tei/odd/p5.nvdl" type="application/xml" schematypens="http://purl.oclc.org/dsdl/nvdl/ns/structure/1.0"?>
<elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="nets" ident="forestGrp">
  <gloss versionDate="2007-07-04" xml:lang="en">forest group</gloss>
  <gloss versionDate="2007-12-20" xml:lang="ko">수형도군</gloss>
  <gloss versionDate="2007-05-02" xml:lang="zh-TW">森林群組</gloss>
  <gloss versionDate="2007-06-12" xml:lang="fr">groupe de forêts</gloss>
  <gloss versionDate="2007-05-04" xml:lang="es">grupo de bosques</gloss>
  <gloss versionDate="2007-01-21" xml:lang="it">gruppo di foreste</gloss>
  <desc versionDate="2005-01-14" xml:lang="en">provides for groups of forests.</desc>
  <desc versionDate="2007-12-20" xml:lang="ko">수형도군을 제시한다.</desc>
  <desc versionDate="2007-05-02" xml:lang="zh-TW">匯集森林群組。</desc>
  <desc versionDate="2008-04-05" xml:lang="ja">森の集合を示す．</desc>
  <desc versionDate="2007-06-12" xml:lang="fr">définit des groupes de forêts.</desc>
  <desc versionDate="2007-05-04" xml:lang="es">define un grupo de bosques</desc>
  <desc versionDate="2007-01-21" xml:lang="it">definisce gruppi di foreste</desc>
  <classes>
    <memberOf key="att.global"/>
    <memberOf key="att.typed"/>
    <memberOf key="model.divPart"/>
  </classes>
  <content>
    <oneOrMore xmlns="http://relaxng.org/ns/structure/1.0">
      <ref name="forest"/>
    </oneOrMore>
  </content>
  <exemplum xml:lang="und">
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
      <forestGrp>
        <forest type="derivation-syntactic">
          <tree ord="true">
            <root children="#fgex1 #fgex2"/>
            <!-- ... -->
          </tree>
          <!-- ... -->
        </forest>
        <forest type="derivation-prosodic">
          <tree ord="true">
            <root children="#fgex3 #fgex4"/>
            <!-- ... -->
          </tree>
          <!-- ... -->
        </forest>
      </forestGrp>
    </egXML>
  </exemplum>
  <exemplum versionDate="2008-04-06" xml:lang="fr">
    <egXML xmlns="http://www.tei-c.org/ns/Examples"> </egXML>
  </exemplum>
  <remarks versionDate="2010-10-20" xml:lang="en">
    <p rend="dataDesc">One or more forests representing the same object.</p>
  </remarks>
  <remarks xml:lang="fr" versionDate="2007-06-12">
    <p rend="dataDesc">Une ou plusieurs forêts.</p>
  </remarks>
  <remarks xml:lang="ja" versionDate="2008-04-05">
    <p rend="dataDesc">
    ひとつ以上の森．
    </p>
  </remarks>
  <listRef>
    <ptr target="#GDAT" type="div2"/>
